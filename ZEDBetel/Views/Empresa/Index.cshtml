@model IEnumerable<VwTabEmpresaVO>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Usuário logado: [@ViewBag.UsuarioLogado.Codigo - @ViewBag.UsuarioLogado.Nome] - [@ViewBag.UsuarioLogado.CodigoEmpresa - @ViewBag.UsuarioLogado.NomeFantasia]</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumeroInscricaoCNPJ)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomeEmpresarial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EnderecoMunicipio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EnderecoUF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomeResponsavel)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumeroInscricaoCNPJ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomeEmpresarial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnderecoMunicipio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnderecoUF)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomeResponsavel)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Codigo }) |
            @Html.ActionLink("Details", "Details", new { id = item.Codigo }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Codigo })
        </td>
    </tr>
}

</table>
